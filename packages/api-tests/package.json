        "@saas-foundry/eslint-config": "workspace:*",
        "@saas-foundry/typescript-config": "workspace:*",
        "@types/node": "^22.13.4",
        "eslint": "^9.20.1",
        "ts-node": "^10.9.2",
        "tsup": "^8.3.6",
        "tsx": "^4.19.3",
        "typescript": "^5.7.3"
